{
  "manifest_version": 2,
  "name": "MojimojiMemoji",
  "version": "1.0.0",

  "description": "나만의 메모를 할 수 있는 메모지 어플",
  "icons": {
    "16": "images/icon16.png"
  },
  "default_locale": "ko",

  "sidebar_action": {
    "default_page": "index.html",
    "default_icon": {
      "16": "images/icon16.png"
    },
    "default_title": "모지모지 메모지"
  },
  "background": {
    "scripts": [],
    "persistent": false,
    "permissions": ["downloads"]
  }
}
